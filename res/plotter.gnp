# gnuplot -c TITLE DATASET PNGNAME

dataset=ARG2

set title ARG1
# set key autotitle columnhead
set datafile sep ','
set xtics 1
set ytics 1000
set yrange [0:10000]

set xlabel 'iteration'
set ylabel '# '.ARG1
set term png size 600, 400
set output ARG3
plot "<(tail -n10 ".dataset.")" u 1:2 w l notitle
